 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"HTML\", \"options\": \"   <h3 style=\\\"text-align: right;\\\"><span style=\\\"line-height: 1.42857;\\\">{{doc.name}}</span></h3>\\n<div>\\n    <hr style=\\\"text-align: center;\\\">\\n</div>   \"}, {\"fieldtype\": \"Section Break\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"employee\"}, {\"print_hide\": 0, \"fieldname\": \"company\"}, {\"print_hide\": 0, \"fieldname\": \"employee_name\"}, {\"print_hide\": 0, \"fieldname\": \"department\"}, {\"print_hide\": 0, \"fieldname\": \"designation\"}, {\"print_hide\": 0, \"fieldname\": \"branch\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"start_date\"}, {\"print_hide\": 0, \"fieldname\": \"end_date\"}, {\"print_hide\": 0, \"fieldname\": \"total_working_days\"}, {\"print_hide\": 0, \"fieldname\": \"leave_without_pay\"}, {\"print_hide\": 0, \"fieldname\": \"payment_days\"}, {\"fieldtype\": \"Section Break\"}, {\"fieldtype\": \"Column Break\"}, {\"visible_columns\": [{\"print_hide\": 0, \"fieldname\": \"salary_component\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"amount\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"depends_on_lwp\", \"print_width\": \"\"}], \"print_hide\": 0, \"fieldname\": \"earnings\"}, {\"fieldtype\": \"Column Break\"}, {\"visible_columns\": [{\"print_hide\": 0, \"fieldname\": \"salary_component\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"amount\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"depends_on_lwp\", \"print_width\": \"\"}], \"print_hide\": 0, \"fieldname\": \"deductions\"}, {\"fieldtype\": \"Section Break\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"gross_pay\"}, {\"print_hide\": 0, \"fieldname\": \"total_deduction\"}, {\"print_hide\": 0, \"fieldname\": \"net_pay\"}, {\"print_hide\": 0, \"fieldname\": \"rounded_total\"}, {\"print_hide\": 0, \"fieldname\": \"total_in_words\"}]", 
 "html": "\n    <div style=\"padding: 15px;\">        \n        <div> \n\t{% if letter_head and not no_letterhead -%}\n            <div class=\"letter-head\" style=\"text-align: center;font-size: 18px;color:#404040;\">\n                {{ letter_head }}\n            </div>\n            <hr>\n    \t{%endif%}\n    \t    </div>\n        <div class=\"page_title\" style=\"\">\n            <h1 class=\"title\" style=\"margin-bottom: 35px;text-align: center;font-size: 24px;color:#404040;\">\n                {{_(\"Salary Slip for the period from {0} to {1}\").format(doc.start_date,doc.end_date)}}</h1>\n        </div>\n\n        <div class=\"personal_info_sec\" style=\"margin-bottom: 30px\">\n            <table style=\"width:100%;border-collapse: collapse;\">\n                <tbody>\n                    <tr>\n                        <td style=\"width:35%;vertical-align: top;padding: 10px;\">\n                            <div class=\"info_container\">\n                                <table style=\"width:100%;border-collapse: collapse;\">\n                                    <tbody>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Employee Name\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">                                \n\t\t\t\t{% if doc.employee_name -%} {{doc.employee_name}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Designation\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">\n                                {% if doc.designation -%} {{_(doc.designation)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Department\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">\n                                {% if doc.department -%} {{_(doc.department)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Circle\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">\n                                {% if doc.circle -%} {{_(doc.circle)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Identity\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.identity -%} {{doc.identity}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Specialization\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.specialization -%} {{_(doc.specialization)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Material Status\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.marital_status -%} {{_(doc.marital_status)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"No of Children\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.no_of_children -%} {{doc.no_of_children}} {% endif -%}</td>\n                                        </tr>\n                                    </tbody>\n                                </table>\n                            </div>\n                        </td>\n                        <td style=\"width:35%;vertical-align: top;padding: 10px;\">\n                            <div class=\"info_container\">\n                                <table style=\"width: 100%;\">\n                                    <tbody>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Date Of Joining\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.date_of_joining -%} {{doc.date_of_joining}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Employement Type\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.employment_type -%} {{_(doc.employment_type)}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Category\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.grade_category -%} {{doc.grade_category}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\"> {{ _(\"Grade\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.grade -%} {{doc.grade}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Experience\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.experience_years -%} {{doc.experience_years}} {% endif -%}</td>\n                                        </tr>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: initial;\">{{ _(\"Hour Cost\") }}</th>\n                                            <td style=\"padding: 5px 10px;\">{% if doc.hour_cost-%} {{doc.hour_cost}} {% endif -%}</td>\n                                        </tr>\n                                    </tbody>\n                                </table>\n                            </div>\n                        </td>\n                        <td style=\"width:30%;vertical-align: top;padding: 10px;\">\n                            <div class=\"person_img\">\n{%if doc.image%}\n                                <img src=\"{{doc.image}}\" style=\"\n                                display: block;\n                                margin: auto;\n                                width: 200px;\n                                height: auto;\n                                max-width: 100%;\n                                -webkit-object-fit: cover;\n                                object-fit: cover;\">\n            {%else%}\n\t            <img src=\"assets/newlinetheme2/images/placeholder-user.png\" style=\"\n                                display: block;\n                                margin: auto;\n                                width: 200px;\n                                height: auto;\n                                max-width: 100%;\n                                -webkit-object-fit: cover;\n                                object-fit: cover;\">\n{%endif%}    \n                            </div>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div><!-- .personal_info_sec -->\n\n        <div class=\"summary_sec\" style=\"margin-bottom: 30px\">\n            <table style=\"width:100%;border-collapse: collapse;\">\n                <tbody>\n                    <tr>\n{% if doc.get(\"earnings\")%}\n                        <td style=\"width:50%;vertical-align: top;padding: 10px;\">\n                            <div class=\"summary_part\">\n                                <h3 class=\"title\" style=\"font-size: 20px;color:#404040;\">{{ _(\"Earnings\") }}</h3>\n                                <table style=\"width:100%;border-collapse: collapse;border: 1px solid #d1d8dd;text-align: center;\" border=\"1px\">\n                                    <tbody>\n                                    <tr>\n                                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_(\"Sr\")}}</th>\n                                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_(\"Component\")}}</th>\n                                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_(\"Amount\")}}</th>\n                                    </tr>\n                    {% for ern in doc.get(\"earnings\")%}\n                                    <tr>\n                                        <td style=\"padding: 5px 10px;\">{{ern.idx}}</td>\n                                        <td style=\"padding: 5px 10px;\">{{_(ern.salary_component)}}</td>                                           \n                                        <td style=\"padding: 5px 10px;\">{{ern.amount}}</td>\n                                    </tr>\n                    {% endfor%}                                                                          \n                                    </tbody>\n                                </table>\n                            </div>\n                        </td>\n{% endif %}\n  {% if doc.get(\"deductions\")%}\n                        <td style=\"width:50%;vertical-align: top;padding: 10px;\">\n                            <div class=\"summary_part\">\n                                <h3 class=\"title\" style=\"font-size: 20px;color:#404040;\">{{ _(\"Deductions\") }}</h3>\n                                <table style=\"width:100%;border-collapse: collapse;border: 1px solid #d1d8dd;text-align: center;\" border=\"1px\">\n                                    <tbody>\n                                        <tr>\n                                           <th style=\"padding: 5px 10px;text-align: center;background-color: #eee;\">{{_(\"Sr\")}}</th>\n                                           <th style=\"padding: 5px 10px;text-align: center;background-color: #eee;\">{{_(\"Component\")}}</th>\n                                           <th style=\"padding: 5px 10px;text-align: center;background-color: #eee;\">{{_(\"Amount\")}}</th>\n                                        </tr>\n                        {% for ded in doc.get(\"deductions\")%}\n                                        <tr>\n                                            <td style=\"padding: 5px 10px;\">{{ded.idx}}</td>\n                                            <td style=\"padding: 5px 10px;\">{{_(ded.salary_component)}}</td>                                           \n                                            <td style=\"padding: 5px 10px;\">{{ded.amount}}</td>\n                                        </tr> \n                        {% endfor%}   \n                                    </tbody>\n                                </table>\n                            </div>\n                        </td>\n{% endif %}\n\n                    </tr>\n                    {% if doc.get(\"loans\") %}\n                    <tr>\n                        <td style=\"width:50%;vertical-align: top;padding: 10px;\">\n                            <div class=\"summary_part\">\n                                <h3 class=\"title\" style=\"font-size: 20px;color:#404040;\">{{_(\"Loans\")}}</h3>\n                                <table style=\"width:100%;border-collapse: collapse;border: 1px solid #d1d8dd;text-align: center;\" border=\"1px\">\n                                    <thead>\n                                        <tr>\n                                            <th style=\"padding: 5px 10px;text-align: center;background-color: #eee;\">{{_(\"Employee Loan\")}}</th>\n                                            <th style=\"padding: 5px 10px;text-align: center;background-color: #eee;\">{{_(\"Total Payment\")}}</th>\n                                        </tr>\n                                    </thead>\n                                    <tbody>\n\n                    {% if doc.get(\"loans\") %}\n                       {% for loan in doc.get('loans')%}\n                                        <tr>\n                                            <td style=\"padding: 5px 10px;\">{{loan.employee_loan}}</td>\n                                            <td style=\"padding: 5px 10px;\">{{loan.total_payment}}</td>\n                                        </tr>\n\n                        {% endfor%} \n                    {% endif %}\n                                    </tbody>\n                                </table>\n                            </div>\n                        </td>                        \n                    </tr>\n{% endif %}\n                </tbody>\n            </table>\n        </div><!-- .summary_sec -->\n\n        <div class=\"total_summary\" style=\"margin-bottom:50px;background-color: #f2f2f2;padding: 15px;\">\n            <h3 class=\"title\" style=\"font-size: 20px;color:#404040;margin-top: 0;\">{{_('Total')}}</h3>\n            <table style=\"width:100%;border-collapse: collapse;text-align: center;\">\n                <tbody >\n                    <tr>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Total Earnings')}}</th>\n                        <td style=\"padding: 5px 10px;\">{{doc.gross_pay}}</td>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\"> {{_('Total Deduction')}}</th>\n                        <td style=\"padding: 5px 10px;\"> {{doc.total_deduction}}</td>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Total Loan')}}</th>\n                        <td style=\"padding: 5px 10px;\">{{doc.total_loan_repayment}}</td>\n                    </tr>\n                    <tr>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">  {{_('Total Overtime')}}</th>\n                        <td style=\"padding: 5px 10px;\">{{doc.total_overtime}}</td>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">  {{_('Rounded Total')}} </th>\n                        <td style=\"padding: 5px 10px;\">{{doc.rounded_total}}</td>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Total')}}</th>\n                        <td style=\"padding: 5px 10px;\">{{doc.net_pay}}</td>\n                    </tr>\n                    <tr>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Total in words')}} </th>\n                        <td colspan=\"5\" style=\"padding: 5px 10px;text-align: initial;\">{{doc.total_in_words}}</td>\n                       \n                    </tr>\n                </tbody>\n            </table>\n        </div><!-- .total_summary -->\n\n        <div class=\"attendance_sec\" style=\"margin-bottom: 30px\">\n            <table style=\"width:100%;border-collapse: collapse;border: 1px solid #d1d8dd;text-align: center;\" border=\"1px\">\n                <thead>\n                    <tr>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('sr')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Date')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">  {{_('Total Hours')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Status')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\"> {{_('Late Hours')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\"> {{_('Overtime Hours')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Early Departure')}} </th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Exit Permissions')}}</th>\n                        <th style=\"padding: 5px 10px;background-color: #eee;\">{{_('Total')}} </th>\n                    </tr>\n                </thead>                       \n                <tbody>      \n        {% for att in doc.get('attendance')%}                         \n                    <tr>\n                        <td style=\"padding: 5px 10px;\">{{att.idx}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.day}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.total_hours}}</td>\n                        <td style=\"padding: 5px 10px;\">{{_(att.status)}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.late_hours}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.overtime}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.early_depature}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.exit_permition}}</td>\n                        <td style=\"padding: 5px 10px;\">{{att.total_working}}</td>  \n                    </tr>   \n \n          {% endfor %}\n                </tbody>\n            </table>\n        </div><!-- .attendance_sec -->\n    </div>\n", 
